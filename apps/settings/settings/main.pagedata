 "body": [
  "<BODY class=\"skin-organic uninit theme-settings\" data-ready=\"true\">\n    \n\n    \n\n    \n    <section role=\"region\" id=\"wifi-status\"><!--\n\n    <gaia-header>\n      <button type=\"reset\" data-icon=\"back\"><span data-l10n-id=\"back\">Back</span></button>\n      <h1 data-ssid=\"\"> Network Status </h1>\n      <button type=\"submit\"><span data-l10n-id=\"forget\">Forget</span></button>\n    </gaia-header>\n\n    <div>\n      <ul>\n        <li>\n          <a>\n            <span data-l10n-id=\"security\">security</span>\n            <span data-security=\"\">WPA-PSK</span>\n          </a>\n        </li>\n        <li>\n          <a>\n            <span data-l10n-id=\"signalStrength\">signal strength</span>\n            <span data-signal=\"\">good</span>\n          </a>\n        </li>\n        <li>\n          <a>\n            <span data-l10n-id=\"ipAddress\">IP Adddress</span>\n            <span data-ip=\"\"></span>\n          </a>\n        </li>\n        <li>\n          <a>\n            <span data-l10n-id=\"linkSpeed\">link speed</span>\n            <span data-speed=\"\"></span>\n          </a>\n        </li>\n      </ul>\n    </div>\n\n    <panel data-path=\"panels/wifi_status/panel\"></panel>\n\n  --></section>\n    \n    <section role=\"region\" id=\"wifi-auth\"><!--\n\n    <gaia-header action=\"back\" data-href=\"#wifi\">\n      <h1 data-ssid=\"\"> Network Authentication </h1>\n      <button type=\"submit\"><span data-l10n-id=\"ok\">OK</span></button>\n    </gaia-header>\n\n    <div>\n      <ul>\n        <li>\n          <a>\n            <span data-l10n-id=\"security\">security</span>\n            <span data-security=\"\">WPA-PSK</span>\n          </a>\n        </li>\n        <li>\n          <a>\n            <span data-l10n-id=\"signalStrength\">signal strength</span>\n            <span data-signal=\"\">good</span>\n          </a>\n        </li>\n        <li class=\"eap\">\n          <p data-l10n-id=\"eap\">EAP method</p>\n          <span class=\"button icon icon-dialog\" data-eapmethod=\"\">\n            <select>\n              <option value=\"SIM\">SIM</option>\n              <option value=\"PEAP\">PEAP</option>\n              <option value=\"TTLS\">TTLS</option>\n            </select>\n          </span>\n        </li>\n        <li class=\"auth-phase2\">\n          <p data-l10n-id=\"auth-phase2\">Phase 2 authentication</p>\n          <span class=\"button icon icon-dialog\">\n            <select>\n              <option value=\"No\">No</option>\n              <option value=\"PAP\">PAP</option>\n              <option value=\"MSCHAP\">MSCHAP</option>\n              <option value=\"MSCHAPV2\">MSCHAP V2</option>\n              <option value=\"GTC\">GTC</option>\n            </select>\n          </span>\n        </li>\n        <li class=\"identity\">\n          <p data-l10n-id=\"wifi-auth-username\">Username</p>\n          <input data-ignore=\"\" name=\"identity\" dir=\"auto\" type=\"text\">\n        </li>\n        <li class=\"password\">\n          <p data-l10n-id=\"wifi-auth-password\">Password</p>\n          <input data-ignore=\"\" name=\"password\" maxlength=\"63\" x-inputmode=\"verbatim\" type=\"password\">\n          <label class=\"pack-checkbox\">\n            <input data-ignore=\"\" name=\"show-pwd\" id=\"pwd-auth\" type=\"checkbox\">\n            <span data-l10n-id=\"showPassword\">show password</span>\n          </label>\n        </li>\n        <li class=\"server-certificate\">\n          <p data-l10n-id=\"server-certificate\">Server certificate</p>\n          <span class=\"button icon icon-dialog\">\n            <select>\n              <option value=\"none\">--</option>\n            </select>\n          </span>\n        </li>\n        <li class=\"description server-certificate-description\">\n          <p data-l10n-id=\"import-certificate-msg\">\n            Notice: If you need to import new certificates, please go to “Manage certificates\".\n          </p>\n        </li>\n      </ul>\n    </div>\n\n    <panel data-path=\"panels/wifi_auth/panel\"></panel>\n\n  --></section>\n    \n    <section role=\"region\" id=\"wifi-joinHidden\"><!--\n\n    <gaia-header>\n      <button type=\"reset\" data-icon=\"back\"><span data-l10n-id=\"back\">Back</span></button>\n      <h1 data-l10n-id=\"authentication-header\"> Authentication </h1>\n      <button type=\"submit\"><span data-l10n-id=\"ok\">OK</span></button>\n    </gaia-header>\n\n    <div>\n      <ul>\n        <li>\n          <p data-l10n-id=\"ssid-name\">SSID Network Name</p>\n          <input data-ignore=\"\" name=\"ssid\" dir=\"auto\" type=\"text\">\n        </li>\n        <li>\n          <p data-l10n-id=\"security\">Security</p>\n          <span class=\"button icon icon-dialog\">\n            <select name=\"security\">\n              <option data-l10n-id=\"securityNone\">none</option>\n              <option>WEP</option>\n              <option selected=\"\">WPA-PSK</option>\n              <option>WPA-EAP</option>\n            </select>\n          </span>\n        </li>\n        <li class=\"eap\">\n          <p data-l10n-id=\"eap\">EAP method</p>\n          <span class=\"button icon icon-dialog\" data-eapmethod=\"\">\n            <select name=\"eap\">\n              <option value=\"SIM\">SIM</option>\n              <option value=\"PEAP\">PEAP</option>\n              <option value=\"TTLS\">TTLS</option>\n            </select>\n          </span>\n        </li>\n        <li class=\"auth-phase2\">\n          <p data-l10n-id=\"auth-phase2\">Phase 2 authentication</p>\n          <span class=\"button icon icon-dialog\">\n            <select>\n              <option value=\"No\">No</option>\n              <option value=\"PAP\">PAP</option>\n              <option value=\"MSCHAP\">MSCHAP</option>\n              <option value=\"MSCHAPV2\">MSCHAP V2</option>\n              <option value=\"GTC\">GTC</option>\n            </select>\n          </span>\n        </li>\n        <li class=\"identity\">\n          <p data-l10n-id=\"wifi-join-username\">Username</p>\n          <input data-ignore=\"\" name=\"identity\" dir=\"auto\" type=\"text\">\n        </li>\n        <li class=\"password\">\n          <p data-l10n-id=\"wifi-join-password\">Password</p>\n          <input data-ignore=\"\" name=\"password\" maxlength=\"63\" type=\"password\">\n          <label class=\"pack-checkbox\">\n            <input data-ignore=\"\" name=\"show-pwd\" id=\"pwd-joinHidden\" type=\"checkbox\">\n            <span data-l10n-id=\"showPassword\">show password</span>\n          </label>\n        </li>\n        <li class=\"server-certificate\">\n          <p data-l10n-id=\"server-certificate\">Server certificate</p>\n          <span class=\"button icon icon-dialog\">\n            <select>\n              <option value=\"none\">--</option>\n            </select>\n          </span>\n        </li>\n        <li class=\"description server-certificate-description\">\n          <p data-l10n-id=\"import-certificate-msg\">\n            Notice: If you need to import new certificates, please go to “Manage certificates\".\n          </p>\n        </li>\n      </ul>\n    </div>\n\n    <panel data-path=\"panels/wifi_join_hidden/panel\"></panel>\n\n  --></section>\n    \n    <section role=\"region\" id=\"wifi-wps\"><!--\n\n    <gaia-header>\n      <button type=\"reset\" data-icon=\"back\"><span data-l10n-id=\"back\">Back</span></button>\n      <h1 data-l10n-id=\"wpsMessage-header\"> Connect with WPS </h1>\n      <button type=\"submit\"><span data-l10n-id=\"ok\">OK</span></button>\n    </gaia-header>\n\n    <div>\n      <header>\n        <h2 data-l10n-id=\"wpsMethodSelection\">Select WPS method:</h2>\n      </header>\n      <ul>\n        <li>\n          <label class=\"pack-radio\">\n            <input data-setting=\"wifi.wps.method\" name=\"wpsMethodSelection\" value=\"pbc\" checked=\"\" type=\"radio\">\n            <span data-l10n-id=\"wpsPbcLabel\">Button connection</span>\n          </label>\n        </li>\n        <li>\n          <label class=\"pack-radio\">\n            <input data-setting=\"wifi.wps.method\" name=\"wpsMethodSelection\" value=\"myPin\" type=\"radio\">\n            <span data-l10n-id=\"wpsMyPinLabel\">My PIN connection</span>\n          </label>\n        </li>\n        <li hidden=\"\">\n          <label class=\"pack-radio\">\n            <input data-setting=\"wifi.wps.method\" name=\"wpsMethodSelection\" value=\"apPin\" type=\"radio\">\n            <span data-l10n-id=\"wpsApPinLabel\">AP PIN connection</span>\n          </label>\n        </li>\n        <li class=\"wifi-wps-pin-area\">\n          <p data-l10n-id=\"wpsPinDescription\">PIN is 4 or 8 digits</p>\n          <input x-inputmode=\"digit\" size=\"8\" maxlength=\"8\" type=\"password\">\n        </li>\n        <li class=\"wifi-wps-pin-aps\">\n          <p data-l10n-id=\"wpsPinAps\">Select an opposite device</p>\n          <span class=\"button icon icon-dialog\">\n            <select></select>\n          </span>\n        </li>\n      </ul>\n    </div>\n\n    <panel data-path=\"panels/wifi_wps/panel\"></panel>\n\n  --></section>\n    \n    <section role=\"region\" id=\"wifi-manageNetworks\"><!--\n\n    <gaia-header>\n      <button type=\"reset\" data-icon=\"back\"><span data-l10n-id=\"back\">Back</span></button>\n      <h1 data-l10n-id=\"manageNetworks-header\"> Manage Networks </h1>\n    </gaia-header>\n\n    <div>\n      <ul>\n        <li>\n          <a>\n            <span data-l10n-id=\"macAddress\"> MAC address </span>\n            <small data-name=\"deviceinfo.mac\"></small>\n          </a>\n        </li>\n      </ul>\n\n      <header>\n        <h2 data-l10n-id=\"knownNetworks\"> Known Networks </h2>\n      </header>\n      <ul class=\"wifi-knownNetworks\">\n      </ul>\n\n      <header>\n        <h2 data-l10n-id=\"hiddenNetworks\"> Hidden Networks </h2>\n      </header>\n      <ul>\n        <li>\n          <a class=\"menu-item joinHidden\" href=\"#\">\n            <span data-l10n-id=\"joinHiddenNetwork\">Join Hidden Networks</span>\n          </a>\n        </li>\n      </ul>\n    </div>\n\n    <form role=\"dialog\" data-type=\"confirm\" hidden=\"\">\n      <section>\n        <h1 data-l10n-id=\"forgetNetwork-confirmation\">Confirmation</h1>\n        <p data-l10n-id=\"forgetNetwork-dialog\">Forget network?</p>\n      </section>\n      <menu>\n        <button data-l10n-id=\"cancel\" type=\"reset\">Cancel</button>\n        <button data-l10n-id=\"device-option-confirm\" id=\"confirm-option\" class=\"danger\">Confirm</button>\n      </menu>\n    </form>\n\n    <panel data-path=\"panels/wifi_manage_networks/panel\"></panel>\n\n  --></section>\n    \n    <section role=\"region\" id=\"wifi-manageCertificates\"><!--\n\n    <gaia-header>\n      <button type=\"reset\" data-icon=\"back\"><span data-l10n-id=\"back\">Back</span></button>\n      <h1 data-l10n-id=\"manageCert\"> Manage Cert. </h1>\n    </gaia-header>\n\n    <div>\n      <header>\n        <h2 data-l10n-id=\"certificateList\"> Certificate List </h2>\n      </header>\n      <ul class=\"wifi-certificateList\">\n      </ul>\n      <ul class=\"operate-certificate\">\n        <li>\n          <button class=\"icon deleteCertificate\" data-l10n-id=\"deleteCertificate\" disabled=\"true\">\n            Delete certificate\n          </button>\n        </li>\n        <li>\n          <a class=\"menu-item importCertificate\" href=\"#\">\n            <span data-l10n-id=\"importCertificate\">Import certificate</span>\n          </a>\n        </li>\n      </ul>\n    </div>\n\n    <form role=\"dialog\" data-type=\"confirm\" class=\"certificate-deletion-failed\" hidden=\"\">\n      <section>\n        <h1 data-l10n-id=\"certificate-deletion-failed\">Certificate deletion failed</h1>\n        <p data-l10n-id=\"certificate-deletion-failed-description\">This may be due to a storage issue, for example.</p>\n      </section>\n      <menu>\n        <button data-l10n-id=\"ok\" class=\"full danger\" id=\"certificate-deletion-failed-confirm\">OK</button>\n      </menu>\n    </form>\n\n    <panel data-path=\"panels/wifi_manage_certificates/panel\"></panel>\n\n  --></section>\n    \n    <section role=\"region\" id=\"wifi-selectCertificateFile\"><!--\n\n    <gaia-header>\n      <button type=\"reset\" data-icon=\"back\"><span data-l10n-id=\"back\">Back</span></button>\n      <h1 data-l10n-id=\"selectCertificateFile\"> Select certificate file </h1>\n    </gaia-header>\n\n    <div>\n      <ul class=\"wifi-certificate-files-List\">\n      </ul>\n    </div>\n\n    <form role=\"dialog\" data-type=\"confirm\" class=\"certificate-import-failed\" hidden=\"\">\n      <section>\n        <h1 data-l10n-id=\"certificate-import-failed\">Certificate import failed</h1>\n        <p data-l10n-id=\"certificate-import-failed-description\">This may be due to a damaged certificate file, for example.</p>\n      </section>\n      <menu>\n        <button data-l10n-id=\"ok\" class=\"full danger\" id=\"certificate-deletion-failed-confirm\">OK</button>\n      </menu>\n    </form>\n\n    <panel data-path=\"panels/wifi_select_certificate_file/panel\"></panel>\n\n  --></section>\n    \n    <section role=\"region\" id=\"wifi-enterCertificateNickname\"><!--\n\n    <gaia-header>\n      <button type=\"reset\" data-icon=\"back\"><span data-l10n-id=\"back\">Back</span></button>\n      <h1 data-l10n-id=\"enterCertificateNickname\"> Enter certificate nick name </h1>\n      <button type=\"submit\"><span data-l10n-id=\"done\">Done</span></button>\n    </gaia-header>\n\n    <div>\n      <ul>\n        <li class=\"nickname\">\n          <p data-l10n-id=\"nickname\">Nick name</p>\n          <input class=\"certificate-file-nickname\" data-ignore=\"\" name=\"nickname\" dir=\"auto\" type=\"text\">\n        </li>\n        <li class=\"description\">\n          <p data-l10n-id=\"enter-certificate-name-msg\">\n            You can change the nick name or keep the default name.\n          </p>\n        </li>\n      </ul>\n    </div>\n\n    <panel data-path=\"panels/wifi_enter_certificate_nickname/panel\"></panel>\n\n  --></section>\n    \n    <section role=\"region\" id=\"wifi\" data-require-sub-panels=\"true\"><!--\n\n    <gaia-header action=\"back\" data-href=\"#root\">\n      <h1 data-l10n-id=\"wifi-header\"> Wi-Fi </h1>\n    </gaia-header>\n\n    <div>\n      <ul>\n        <li class=\"wifi-enabled\">\n          <label class=\"pack-switch\">\n            <input data-ignore=\"\" type=\"checkbox\">\n            <span data-l10n-id=\"wifi\">Wi-Fi</span>\n          </label>\n        </li>\n        <li hidden=\"\">\n          <label class=\"pack-checkbox\">\n            <input name=\"wifi.notification\" type=\"checkbox\">\n            <span data-l10n-id=\"networkNotification\">Network notification</span>\n            <small data-l10n-id=\"networkNotification-expl\">Notify me when an open network is available</small>\n          </label>\n        </li>\n        <li class=\"wps-column\">\n          <a class=\"menu-item\" href=\"#\">\n            <span data-l10n-id=\"wpsMessage\">Connect with WPS</span>\n            <small data-l10n-id=\"wpsDescription2\">Automatic Wi-Fi setup</small>\n          </a>\n        </li>\n      </ul>\n\n      <header>\n        <h2 data-l10n-id=\"availableNetworks\"> Available Networks </h2>\n      </header>\n      <ul class=\"wifi-availableNetworks\" data-state=\"off\">\n        <li class=\"explanation\" data-state=\"off\" data-l10n-id=\"wifi-disabled\">\n          Turn Wi-Fi on to view available networks.\n        </li>\n        <li data-state=\"on\">\n          <progress></progress><span data-l10n-id=\"scanning\">Searching…</span>\n        </li>\n        <li data-state=\"ready\">\n          <button data-l10n-id=\"scanNetworks\">Search Again</button>\n        </li>\n      </ul>\n\n      <header>\n        <h2 data-l10n-id=\"advancedSettings\"> Advanced Settings </h2>\n      </header>\n      <ul>\n        <li>\n          <a class=\"menu-item manageNetworks\" href=\"#\">\n            <span data-l10n-id=\"manageNetworks\">Manage networks</span>\n          </a>\n        </li>\n        <li>\n          <a class=\"menu-item manageCertificates\" href=\"#\">\n            <span data-l10n-id=\"manageCertificates\">Manage certificates</span>\n          </a>\n        </li>\n      </ul>\n    </div>\n\n    <panel data-path=\"panels/wifi/panel\"></panel>\n\n    <form role=\"dialog\" data-type=\"confirm\" class=\"wifi-bad-credentials-dialog\" hidden=\"\">\n      <section>\n        <h1 data-l10n-id=\"wifi-bad-credentials-title\">Failed connecting to Wi-Fi</h1>\n        <p data-l10n-id=\"wifi-bad-credentials-confirm\">\n          Unable to connect to {{ssid}}, authentication failed due to bad credentials. Please re-enter the network password.\n        </p>\n      </section>\n      <menu>\n        <button data-l10n-id=\"cancel\" class=\"wifi-bad-credentials-cancel\">Cancel</button>\n        <button data-l10n-id=\"ok\" class=\"wifi-bad-credentials-confirm recommend\">Ok</button>\n      </menu>\n    </form>\n  --></section>\n    \n    <section role=\"region\" id=\"call-cf-notReachableSettings\"><!--\n\n    <gaia-header>\n      <button type=\"reset\" data-icon=\"back\"><span data-l10n-id=\"back\">Back</span></button>\n      <h1 data-l10n-id=\"callForwarding-header\"> Call forwarding </h1>\n      <button type=\"submit\"><span data-l10n-id=\"ok\">OK</span></button>\n    </gaia-header>\n\n    <div>\n      <ul id=\"cf-notreachable-advanced\">\n        <li>\n          <p data-l10n-id=\"cf-numberWhenUnreachable\">Number when unreachable</p>\n          <input id=\"cf-notreachable-number\" data-setting=\"ril.cf.notreachable.number\" type=\"tel\">\n        </li>\n        <li>\n          <label class=\"pack-switch\">\n            <input data-setting=\"ril.cf.notreachable.enabled\" type=\"checkbox\">\n            <span data-l10n-id=\"callForwardingForward\">Forward</span>\n            <small data-l10n-id=\"callForwardingNotReachableShort\">When unreachable</small>\n          </label>\n        </li>\n      </ul>\n    </div>\n\n  --></section>\n    \n    <section role=\"region\" id=\"call-cf-noReplySettings\"><!--\n\n    <gaia-header>\n      <button type=\"reset\" data-icon=\"back\"><span data-l10n-id=\"back\">Back</span></button>\n      <h1 data-l10n-id=\"callForwarding-header\"> Call forwarding </h1>\n      <button type=\"submit\"><span data-l10n-id=\"ok\">OK</span></button>\n    </gaia-header>\n\n    <div>\n      <ul id=\"cf-noreply-advanced\">\n        <li>\n          <p data-l10n-id=\"cf-numberWhenUnanswered\">Number when unanswered</p>\n          <input id=\"cf-noreply-number\" data-setting=\"ril.cf.noreply.number\" type=\"tel\">\n        </li>\n        <li>\n          <label class=\"pack-switch\">\n            <input data-setting=\"ril.cf.noreply.enabled\" type=\"checkbox\">\n            <span data-l10n-id=\"callForwardingForward\">Forward</span>\n            <small data-l10n-id=\"callForwardingNoReplyShort\">When unanswered</small>\n          </label>\n        </li>\n      </ul>\n    </div>\n\n  --></section>\n    \n    <section role=\"region\" id=\"call-cf-mobileBusySettings\"><!--\n\n    <gaia-header>\n      <button type=\"reset\" data-icon=\"back\"><span data-l10n-id=\"back\">Back</span></button>\n      <h1 data-l10n-id=\"callForwarding-header\"> Call Forwarding </h1>\n      <button type=\"submit\"><span data-l10n-id=\"ok\">OK</span></button>\n    </gaia-header>\n\n    <div>\n      <ul id=\"cf-mobilebusy-advanced\">\n        <li>\n          <p data-l10n-id=\"cf-numberWhenBusy\">Number when busy</p>\n          <input id=\"cf-mobilebusy-number\" data-setting=\"ril.cf.mobilebusy.number\" type=\"tel\">\n        </li>\n        <li>\n          <label class=\"pack-switch\">\n            <input data-setting=\"ril.cf.mobilebusy.enabled\" type=\"checkbox\">\n            <span data-l10n-id=\"callForwardingForward\">Forward</span>\n            <small data-l10n-id=\"callForwardingMobileBusyShort\">When busy</small>\n          </label>\n        </li>\n      </ul>\n    </div>\n\n  --></section>\n    \n    <section role=\"region\" id=\"call-cf-unconditionalSettings\"><!--\n\n    <gaia-header>\n      <button type=\"reset\" data-icon=\"back\"><span data-l10n-id=\"back\">Back</span></button>\n      <h1 data-l10n-id=\"callForwarding-header\"> Call forwarding </h1>\n      <button type=\"submit\"><span data-l10n-id=\"ok\">OK</span></button>\n    </gaia-header>\n\n    <div>\n      <ul id=\"cf-unconditional-advanced\">\n        <li>\n          <p data-l10n-id=\"cf-alwaysUseThisNumber\">Always use this number</p>\n          <input id=\"cf-unconditional-number\" data-setting=\"ril.cf.unconditional.number\" type=\"tel\">\n        </li>\n        <li>\n          <label class=\"pack-switch\">\n            <input data-setting=\"ril.cf.unconditional.enabled\" type=\"checkbox\">\n            <span data-l10n-id=\"callForwardingForward\">Forward</span>\n            <small data-l10n-id=\"callForwardingUnconditionalShort\">Always</small>\n          </label>\n        </li>\n      </ul>\n    </div>\n\n  --></section>\n    \n    <section role=\"region\" id=\"call-fdnList-add\"><!--\n\n    <gaia-header action=\"back\" data-href=\"#call-fdnList\">\n      <h1 id=\"fdnContact-title\"></h1>\n      <button id=\"fdnContact-submit\"><span data-l10n-id=\"done\">Done</span></button>\n    </gaia-header>\n\n    <div>\n      <ul>\n        <li>\n          <p data-l10n-id=\"fdnContact-name\">Name</p>\n          <input x-inputmode=\"verbatim\" id=\"fdnContact-name\" dir=\"auto\" type=\"text\">\n        </li>\n        <li>\n          <p data-l10n-id=\"fdnContact-number\">Number</p>\n          <input x-inputmode=\"verbatim\" id=\"fdnContact-number\" type=\"tel\">\n        </li>\n      </ul>\n    </div>\n\n  --></section>\n    \n    <section role=\"region\" id=\"call-fdnList\" data-require-sub-panels=\"true\"><!--\n\n    <gaia-header action=\"back\" data-href=\"#call-fdnSettings\">\n      <h1 data-l10n-id=\"fdn-authorizedNumbers-header\"> Authorized Numbers </h1>\n      <button id=\"fdnContact\"><span data-l10n-id=\"add\" class=\"icon icon-add\">Add</span></button>\n    </gaia-header>\n\n    <form id=\"call-fdnList-action\" role=\"dialog\" data-type=\"action\" hidden=\"\">\n      <header>\n        <span id=\"fdnAction-name\">John Doe</span><br>\n        <span id=\"fdnAction-number\">+1 2345678</span>\n      </header>\n      <menu>\n        <button id=\"fdnAction-call\" data-l10n-id=\"fdnAction-call\"> Call </button>\n        <button id=\"fdnAction-edit\" data-l10n-id=\"fdnAction-edit\"> Edit contact </button>\n        <button id=\"fdnAction-delete\" data-l10n-id=\"fdnAction-delete\"> Delete contact </button>\n        <button id=\"fdnAction-cancel\" data-l10n-id=\"cancel\"> Cancel </button>\n      </menu>\n    </form>\n\n    <div>\n      <ul id=\"fdn-contactsContainer\"></ul>\n    </div>\n\n    <script src=\"js/simcard_fdn_list.js\"></script>\n\n  --></section>\n    \n    <section role=\"region\" id=\"call-pin2-dialog\"><!--\n\n    <gaia-header>\n      <button type=\"reset\" data-icon=\"back\"><span data-l10n-id=\"back\">Back</span></button>\n      <h1></h1>\n      <button data-l10n-id=\"done\" type=\"submit\">Done</button>\n    </gaia-header>\n\n    <div class=\"pin-dialog\">\n      <div class=\"sim-errorMsg error\" hidden=\"\">\n        <div class=\"sim-messageHeader\"></div>\n        <span class=\"sim-messageBody\"></span>\n      </div>\n      <div class=\"sim-triesLeft\" data-l10-id=\"inputCodeRetriesLeft\" hidden=\"\"></div>\n\n      <div class=\"sim-code-area sim-pinArea\">\n        <div data-l10n-id=\"simPin2\">SIM PIN2</div>\n        <input x-inputmode=\"digit\" size=\"8\" maxlength=\"8\" type=\"password\">\n      </div>\n\n      <div class=\"sim-code-area sim-pukArea\">\n        <div data-l10n-id=\"puk2Code\">PUK2 Code</div>\n        <input x-inputmode=\"digit\" size=\"8\" maxlength=\"8\" type=\"password\">\n      </div>\n\n      <div class=\"sim-code-area sim-newPinArea\">\n        <div data-l10n-id=\"newSimPin2Msg\">\n          Create PIN2 (must