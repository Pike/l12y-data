region\" id=\"call-voiceMailSettings\"><!--\n\n    <gaia-header action=\"back\" data-href=\"#call\">\n      <h1 data-l10n-id=\"voiceMail-header\"> Voicemail </h1>\n      <button type=\"submit\"><span data-l10n-id=\"ok\">OK</span></button>\n    </gaia-header>\n\n    <div>\n      <ul>\n        <li>\n          <p data-l10n-id=\"voiceMail-number\">Voicemail Number</p>\n          <input id=\"vm-number\" type=\"tel\">\n        </li>\n      </ul>\n    </div>\n\n    <script src=\"js/call_voicemail_settings.js\"></script>\n  --></section>\n    \n    <section role=\"region\" id=\"call-iccs\"><!--\n\n    <gaia-header action=\"back\" data-href=\"#root\">\n      <h1 data-l10n-id=\"selectASIMCard\"> Select a SIM card </h1>\n    </gaia-header>\n\n    <div>\n      <ul>\n        <li id=\"menuItem-call-sim1\" aria-disabled=\"true\">\n          <a class=\"menu-item\" href=\"#call\">\n            <span data-l10n-id=\"sim1\">SIM 1</span>\n            <small></small>\n          </a>\n        </li>\n        <li id=\"menuItem-call-sim2\" aria-disabled=\"true\">\n          <a class=\"menu-item\" href=\"#call\">\n            <span data-l10n-id=\"sim2\">SIM 2</span>\n            <small></small>\n          </a>\n        </li>\n      </ul>\n    </div>\n\n    <script src=\"js/call_iccs.js\"></script>\n\n  --></section>\n    \n    <section role=\"region\" id=\"call\" data-require-sub-panels=\"true\"><!--\n\n    <gaia-header action=\"back\" data-href=\"#root\">\n      <h1 data-l10n-id=\"callSettings-header\"> Call Settings </h1>\n    </gaia-header>\n\n    <div>\n      <ul>\n        <li>\n          <a class=\"menu-item\" href=\"#\" data-href=\"#call-voiceMailSettings\">\n            <span data-l10n-id=\"voiceMail\">Voicemail</span>\n            <small id=\"voiceMail-desc\"></small>\n          </a>\n        </li>\n        <li id=\"menuItem-voicePrivacyMode\" hidden=\"\">\n          <label class=\"pack-switch checkbox-label\">\n            <input data-ignore=\"\" type=\"checkbox\">\n            <span data-l10n-id=\"voicePrivacyMode\">Voice Privacy Mode</span>\n          </label>\n        </li>\n        <li id=\"menuItem-callWaiting\" data-state=\"off\" hidden=\"\">\n          <label class=\"pack-switch checkbox-label\">\n            <input data-ignore=\"\" type=\"checkbox\">\n            <span data-l10n-id=\"callWaiting\">Call Waiting</span>\n          </label>\n          <label class=\"alert-label\" hidden=\"\">\n            <span></span>\n          </label>\n        </li>\n        <li id=\"menuItem-callerId\" aria-disabled=\"true\" hidden=\"\">\n          <a data-l10n-id=\"callerId\">Caller ID</a>\n          <span class=\"button icon icon-dialog\">\n            <select id=\"ril-callerId\" data-setting=\"ril.callerId\">\n              <option value=\"CLIR_DEFAULT\" data-l10n-id=\"callerId-default\">Network default</option>\n              <option value=\"CLIR_INVOCATION\" data-l10n-id=\"callerId-hide\">Hide number</option>\n              <option value=\"CLIR_SUPPRESSION\" data-l10n-id=\"callerId-show\">Show number</option>\n            </select>\n          </span>\n        </li>\n        <li>\n          <a class=\"menu-item\" href=\"#\" data-href=\"#call-fdnSettings\">\n            <span data-l10n-id=\"fdnSettings\">Fixed Dialing Numbers</span>\n            <small id=\"fdnSettings-desc\"></small>\n          </a>\n        </li>\n      </ul>\n\n      <header id=\"header-callForwarding\" hidden=\"\">\n        <h2 data-l10n-id=\"callForwarding\"> Call Forwarding </h2>\n      </header>\n      <ul id=\"list-callForwarding\" hidden=\"\">\n        <li id=\"fdnSettingsBlocked\" class=\"description\" hidden=\"\">\n          <p data-l10n-id=\"fdn-enabled-warning\">\n            Call forwarding is disabled by FDN.\n          </p>\n        </li>\n        <li id=\"li-cfu-desc\" aria-disabled=\"true\">\n          <a class=\"menu-item\" id=\"menuItem-callForwardingUnconditional\" href=\"#\" data-href=\"#call-cf-unconditionalSettings\">\n            <span data-l10n-id=\"callForwardingUnconditional\">Always forward</span>\n            <small id=\"cfu-desc\"></small>\n          </a>\n        </li>\n        <li id=\"li-cfmb-desc\" aria-disabled=\"true\">\n          <a class=\"menu-item\" id=\"menuItem-callForwardingMobileBusy\" href=\"#\" data-href=\"#call-cf-mobileBusySettings\">\n            <span data-l10n-id=\"callForwardingMobileBusy\">Forward when busy</span>\n            <small id=\"cfmb-desc\"></small>\n          </a>\n        </li>\n        <li id=\"li-cfnrep-desc\" aria-disabled=\"true\">\n          <a class=\"menu-item\" id=\"menuItem-callForwardingNoReply\" href=\"#\" data-href=\"#call-cf-noReplySettings\">\n            <span data-l10n-id=\"callForwardingNoReply\">Forward when unanswered</span>\n            <small id=\"cfnrep-desc\"></small>\n          </a>\n        </li>\n        <li id=\"li-cfnrea-desc\" aria-disabled=\"true\">\n          <a class=\"menu-item\" id=\"menuItem-callForwardingNotReachable\" href=\"#\" data-href=\"#call-cf-notReachableSettings\">\n            <span data-l10n-id=\"callForwardingNotReachable\">Forward when unreachable</span>\n            <small id=\"cfnrea-desc\"></small>\n          </a>\n        </li>\n      </ul>\n    </div>\n\n    <form role=\"dialog\" data-type=\"confirm\" class=\"cw-alert\" hidden=\"\">\n      <section>\n        <h1 data-l10n-id=\"confirmCallWaitingTitle\">Unable to confirm call waiting preferences</h1>\n        <p>\n          <strong data-l10n-id=\"confirmCallWaitingDesc\">The device is currently unable to communicate with the carrier. Try setting the preference again.</strong>\n        </p>\n        <p class=\"cw-alert-sub-p\">\n          <label class=\"pack-switch cw-alert-checkbox-label\">\n            <input data-ignore=\"\" type=\"checkbox\">\n            <span data-l10n-id=\"callWaiting\">Call Waiting</span>\n          </label>\n        </p>\n      </section>\n      <menu>\n        <button class=\"cw-alert-cancel\" data-l10n-id=\"cancel\">Cancel</button>\n        <button class=\"cw-alert-set recommend\" data-l10n-id=\"set\">Set</button>\n      </menu>\n    </form>\n\n    <form role=\"dialog\" data-type=\"confirm\" class=\"cf-alert\" hidden=\"\">\n      <section>\n        <h1 data-l10n-id=\"callForwardingConfirmTitle\">Call forwarding confirmation message</h1>\n        <p>\n          <strong id=\"cf-confirm-message\"></strong>\n        </p>\n      </section>\n      <menu>\n        <button class=\"cf-alert-continue full\" data-l10n-id=\"continue\">Continue</button>\n      </menu>\n    </form>\n\n    <script src=\"shared/js/settings_helper.js\"></script>\n    <script src=\"js/call.js\"></script>\n\n  --></section>\n    \n    <section role=\"region\" id=\"messaging-details\"><!--\n\n    <gaia-header action=\"back\" data-href=\"#messaging\">\n      <h1 data-l10n-id=\"messaging-sim-settings\">SIM Settings</h1>\n    </gaia-header>\n\n    <div class=\"options-container\"></div>\n\n    <panel data-path=\"panels/messaging_details/panel\"></panel>\n\n  --></section>\n    \n    <section role=\"region\" id=\"messaging\"><!--\n\n    <gaia-header action=\"back\" data-href=\"#root\">\n      <h1 data-l10n-id=\"messagingSettings-header\"> Messaging Settings </h1>\n    </gaia-header>\n\n    <div>\n      <header>\n        <h2 data-l10n-id=\"message-confirmation\">Message Confirmation</h2>\n      </header>\n      <ul id=\"general-message-list\">\n        <li id=\"menuItem-deliveryReport\" aria-disabled=\"true\" class=\"hint\">\n          <label class=\"pack-switch\">\n            <input name=\"ril.sms.requestStatusReport.enabled\" type=\"checkbox\">\n            <span data-l10n-id=\"message-delivery-reports\">Delivery Reports</span>\n          </label>\n          <p class=\"explanation\" data-l10n-id=\"message-delivery-reports-details\">Request notification of delivery for each message sent</p>\n        </li>\n      </ul>\n      <header>\n        <h2 data-l10n-id=\"mmsTitle\">Multimedia Messaging (MMS)</h2>\n      </header>\n      <ul class=\"mmsSettings